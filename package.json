{
  "name": "picgo-for-vscode",
  "displayName": "PicGo for VSCode extension",
  "description": "An extension for the VSCode IDE that enables users to upload images to remote hosts, including Aliyun OSS, Tencent Cloud COS, Qiuniu Image Host, Github, s.ee (sm.ms), imgurl, etc.",
  "version": "1.0.1",
  "publisher": "longmix",
  "license": "MIT",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/longmix/PicGo-for-VSCode.git"
  },
  "main": "./dist/extension.js",
  "scripts": {
    "compile": "webpack --mode production",
    "package": "npm run compile && vsce package",
    "vscode:prepublish001": "npm run compile",
    "compile001": "tsc -p ./",
    "watch001": "tsc -watch -p ./",
    "lint001": "eslint src --ext ts"
  },
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:markdown",
    "onCommand:picgo-for-vscode.uploadFromClipboard",
    "onCommand:picgo-for-vscode.uploadFromFinder",
    "onCommand:picgo-for-vscode.uploadFromInput",
    "onCommand:picgo-for-vscode.webviewDemo",
    "onCommand:picgo-for-vscode.webviewPicGoControlPanel",
    "workspaceContains:vs-picgo-auto-launch.txt"
  ],
  "contributes": {
    "localizations": [
      {
        "language": "en",
        "translations": [
          {
            "id": "vscode",
            "path": "./package.nls.json"
          }
        ]
      },
      {
        "language": "zh-cn",
        "translations": [
          {
            "id": "vscode",
            "path": "./package.nls.zh-cn.json"
          }
        ]
      },
      {
        "language": "zh-tw",
        "translations": [
          {
            "id": "vscode",
            "path": "./package.nls.zh-tw.json"
          }
        ]
      }
    ],
    "commands": [
      {
        "command": "picgo-for-vscode.uploadFromClipboard",
        "title": "markdown: Upload and Paste Image from Clipboard"
      },
      {
        "command": "picgo-for-vscode.uploadFromFinder",
        "title": "markdown: Upload and Paste Image by Choosing File"
      },
      {
        "command": "picgo-for-vscode.uploadFromInput",
        "title": "markdown: Upload and Paste Image from Input Path"
      }
    ],
    "keybindings": [
      {
        "command": "picgo-for-vscode.uploadFromClipboard",
        "key": "ctrl+alt+u",
        "mac": "cmd+alt+u",
        "when": "editorTextFocus && editorLangId == markdown"
      },
      {
        "command": "picgo-for-vscode.uploadFromFinder",
        "key": "ctrl+alt+e",
        "mac": "cmd+alt+e",
        "when": "editorTextFocus && editorLangId == markdown"
      },
      {
        "command": "picgo-for-vscode.uploadFromInput",
        "key": "ctrl+alt+o",
        "mac": "cmd+alt+o",
        "when": "editorTextFocus && editorLangId == markdown"
      }
    ],
    "configuration": {
      "title": "Markdown Image Paste",
      "properties": {
        "markdown-image-paste.picBed.uploader": {
          "type": "string",
          "enum": [
            "smms",
            "aliyun",
            "github",
            "imgur",
            "qiniu",
            "tcyun",
            "upyun",
            "weibo"
          ],
          "default": "",
          "markdownDescription": "%config.picBed.uploader%"
        },
        "markdown-image-paste.picBed.current": {
          "type": "string",
          "enum": [
            "smms",
            "aliyun",
            "github",
            "imgur",
            "qiniu",
            "tcyun",
            "upyun",
            "weibo"
          ],
          "default": "smms",
          "markdownDescription": "%config.picBed.current%"
        },
        "markdown-image-paste.picBed.proxy": {
          "type": "string",
          "default": "",
          "markdownDescription": "%config.picBed.proxy%"
        },
        "markdown-image-paste.picBed.smms.token": {
          "type": "string",
          "default": ""
        },
        "markdown-image-paste.picBed.aliyun.accessKeyId": {
          "type": "string",
          "default": ""
        },
        "markdown-image-paste.picBed.aliyun.accessKeySecret": {
          "type": "string",
          "default": ""
        },
        "markdown-image-paste.picBed.aliyun.bucket": {
          "type": "string",
          "default": ""
        },
        "markdown-image-paste.picBed.aliyun.area": {
          "type": "string",
          "default": ""
        },
        "markdown-image-paste.picBed.aliyun.path": {
          "type": "string",
          "default": ""
        },
        "markdown-image-paste.picBed.aliyun.customUrl": {
          "type": "string",
          "default": ""
        },
        "markdown-image-paste.picBed.github.repo": {
          "type": "string",
          "default": "",
          "markdownDescription": "%config.picBed.github.repo.description%"
        },
        "markdown-image-paste.picBed.github.token": {
          "type": "string",
          "default": ""
        },
        "markdown-image-paste.picBed.github.path": {
          "type": "string",
          "default": ""
        },
        "markdown-image-paste.picBed.github.customUrl": {
          "type": "string",
          "default": ""
        },
        "markdown-image-paste.picBed.github.branch": {
          "type": "string",
          "default": ""
        },
        "markdown-image-paste.picBed.imgur.clientId": {
          "type": "string",
          "default": ""
        },
        "markdown-image-paste.picBed.imgur.proxy": {
          "type": "string",
          "default": ""
        },
        "markdown-image-paste.picBed.qiniu.accessKey": {
          "type": "string",
          "default": ""
        },
        "markdown-image-paste.picBed.qiniu.secretKey": {
          "type": "string",
          "default": ""
        },
        "markdown-image-paste.picBed.qiniu.bucket": {
          "type": "string",
          "default": ""
        },
        "markdown-image-paste.picBed.qiniu.url": {
          "type": "string",
          "default": ""
        },
        "markdown-image-paste.picBed.qiniu.area": {
          "type": "string",
          "enum": [
            "z0",
            "z1",
            "z2",
            "na0",
            "as0"
          ],
          "default": "z0"
        },
        "markdown-image-paste.picBed.qiniu.options": {
          "type": "string",
          "default": ""
        },
        "markdown-image-paste.picBed.qiniu.path": {
          "type": "string",
          "default": ""
        },
        "markdown-image-paste.picBed.tcyun.version": {
          "type": "string",
          "enum": [
            "v4",
            "v5"
          ],
          "default": "v5"
        },
        "markdown-image-paste.picBed.tcyun.secretId": {
          "type": "string",
          "default": ""
        },
        "markdown-image-paste.picBed.tcyun.secretKey": {
          "type": "string",
          "default": ""
        },
        "markdown-image-paste.picBed.tcyun.bucket": {
          "type": "string",
          "default": ""
        },
        "markdown-image-paste.picBed.tcyun.appId": {
          "type": "string",
          "default": ""
        },
        "markdown-image-paste.picBed.tcyun.area": {
          "type": "string",
          "default": ""
        },
        "markdown-image-paste.picBed.tcyun.path": {
          "type": "string",
          "default": ""
        },
        "markdown-image-paste.picBed.tcyun.customUrl": {
          "type": "string",
          "default": ""
        },
        "markdown-image-paste.picBed.upyun.bucket": {
          "type": "string",
          "default": ""
        },
        "markdown-image-paste.picBed.upyun.operator": {
          "type": "string",
          "default": ""
        },
        "markdown-image-paste.picBed.upyun.password": {
          "type": "string",
          "default": ""
        },
        "markdown-image-paste.picBed.upyun.options": {
          "type": "string",
          "default": ""
        },
        "markdown-image-paste.picBed.upyun.path": {
          "type": "string",
          "default": ""
        },
        "markdown-image-paste.picBed.upyun.url": {
          "type": "string",
          "default": ""
        },
        "markdown-image-paste.picBed.weibo.chooseCookie": {
          "type": "boolean",
          "default": true
        },
        "markdown-image-paste.picBed.weibo.username": {
          "type": "string",
          "default": ""
        },
        "markdown-image-paste.picBed.weibo.quality": {
          "type": "string",
          "enum": [
            "thumbnail",
            "mw690",
            "large"
          ],
          "default": "large"
        },
        "markdown-image-paste.picBed.weibo.cookie": {
          "type": "string",
          "default": ""
        },
        "markdown-image-paste.autoUploadOnPaste": {
          "type": "boolean",
          "default": true,
          "description": "%config.autoUploadOnPaste%"
        }
      }
    }
  },
  "devDependencies": {
    "@types/node": "^18.x",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vscode/vsce": "^3.7.1",
    "eslint": "^8.0.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.0.0",
    "webpack": "^5.105.2",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "picgo": "^2.0.2"
  }
}
